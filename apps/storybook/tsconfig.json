    "declaration": false,
    "declarationMap": false,
    "emitDeclarationOnly": false,
    "types": ["node", "vite/client"],
    "jsx": "react-jsx",
    "lib": ["ES2022", "DOM", "DOM.Iterable"]
  },
  "include": [
    ".storybook/**/*",
    "stories/**/*"
  ]
}

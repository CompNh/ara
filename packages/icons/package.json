{
  "name": "@ara/icons",
  "version": "0.0.0",
  "description": "Ara 디자인 시스템을 위한 SVG 아이콘 모음",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "default": "./dist/*.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "icons/*": [
        "dist/icons/*.d.ts"
      ],
      "types": [
        "dist/types.d.ts"
      ],
      "*": [
        "dist/*.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "pnpm exec rollup -c",
    "svgo": "pnpm exec svgo -c svgo.config.mjs",
    "lint": "pnpm exec eslint src --ext .ts,.tsx",
    "check:exports": "node ./scripts/verify-exports.mjs",
    "pack:dry-run": "npm pack --dry-run --json"
  },
  "keywords": [
    "icons",
    "svg",
    "design system"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/CompNh/ara.git"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@ara/tsconfig": "workspace:*",
    "@types/react": "^18.3.12",
    "react": "^18.2.0",
    "svgo": "3"
  }
}
